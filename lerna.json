{
  "packages": [
    "packages/**"
  ],
  "npmClient": "yarn",
  "useWorkspaces": true,
  "version": "independent",
  "ignoreChanges": [
    "**/__mocks__/**",
    "**/__tests__/**",
    "**/*.spec.js",
    "**/*.stories.mdx",
    "**/*.md"
  ],
  "command": {
    "publish": {
      "registry": "https://registry.npmjs.org/"
    }
  }
}
